{
  "version": 2,
  "public": true,
  "trailingSlash": true,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/pdfspark/:path*",
      "destination": "/pdfspark/index.html"
    }
  ],
  "headers": [
    {
      "source": "/pdfspark/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/pdfspark/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}