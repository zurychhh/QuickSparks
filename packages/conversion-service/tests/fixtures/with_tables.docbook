  </section>
  
  <section>
    <title>Schedule Table</title>
    <para>A table representing a weekly schedule:</para>
    <table frame="all">
      <title>Weekly Schedule</title>
      <tgroup cols="6">
        <thead>
          <row>
            <entry>Time</entry>
            <entry>Monday</entry>
            <entry>Tuesday</entry>
            <entry>Wednesday</entry>
            <entry>Thursday</entry>
            <entry>Friday</entry>
          </row>
        </thead>
        <tbody>
          <row>
            <entry>9:00 - 10:30</entry>
            <entry>Team Meeting</entry>
            <entry>Client Call</entry>
            <entry>Development</entry>
            <entry>Planning</entry>
            <entry>Development</entry>
          </row>
          <row>
            <entry>10:45 - 12:15</entry>
            <entry>Development</entry>
            <entry>Development</entry>
            <entry>Design Review</entry>
            <entry>Development</entry>
            <entry>Team Meeting</entry>
          </row>
          <row>
            <entry>13:00 - 14:30</entry>
            <entry>Design Meeting</entry>
            <entry>Development</entry>
            <entry>Client Demo</entry>
            <entry>Development</entry>
            <entry>Project Review</entry>
          </row>
          <row>
            <entry>14:45 - 16:15</entry>
            <entry>Development</entry>
            <entry>Testing</entry>
            <entry>Development</entry>
            <entry>Client Call</entry>
