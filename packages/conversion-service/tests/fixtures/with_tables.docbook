          </row>
          <row>
            <entry><emphasis role="bold">Total</emphasis></entry>
            <entry><emphasis role="bold">$729,000</emphasis></entry>
            <entry><emphasis role="bold">$849,000</emphasis></entry>
            <entry><emphasis role="bold">$891,000</emphasis></entry>
